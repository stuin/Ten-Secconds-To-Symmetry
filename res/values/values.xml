<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">Ten Seconds To Symmetry</string>
    <string name="app_start">Start Game</string>

    <string-array name="game_labels">
        <item>Next Round</item>
        <item>Restart</item>
        <item>New High Score</item>
        <item>High Score:" "</item>
        <item>Game Lost:" "</item>
        <item>Complete:" "</item>
    </string-array>

    <style name="style_base">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="style_norm" parent="style_base">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="style_title" parent="style_base">
        <item name="android:textSize">25sp</item>
    </style>
</resources>